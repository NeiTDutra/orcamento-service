extends ../orcamento/orcamento_index

block rps-list

    script.
        $(document).ready(function(){
        $('table tr').click(function(){
            window.location = $(this).data('url');
            return false;
            });
        });

    h3.pt-2.pb-2.pl-2.bg-warning Lista de RPS - #[strong #{title}]

    each rps in rps_list
        p #[storng Data:] #{rps.dataHora}
        p #[strong Numero:] #{rps.numero}
        p #[strong Série:] #{rps.serie}
        p #[strong Prestador:]
        ul
            li cpf: #{rps.prestador.cpf}
            li cnpj: #{rps.prestador.cnpj}
            li isnc. municipal: #{rps.prestador.inscr_municipal}
            li nome/razão: #{rps.prestador.nome_razao}
            li #[strong endereço:] 
            ul 
                li cep: #{rps.prestador.endereco.cep}
                li logradouro: #{rps.prestador.endereco.logradouro}